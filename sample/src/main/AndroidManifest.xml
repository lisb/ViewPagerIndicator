<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lisb.viewpageindicator3.sample">

    <application
        android:label="ViewPagerIndicator Sample"
        android:icon="@drawable/icon">

        <activity
            android:name="com.lisb.viewpageindicator3.sample.ListSamples"
            android:label="ViewPager Indicator">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- ICON INDICATOR -->
        <activity
            android:name="com.lisb.viewpageindicator3.sample.SampleIconsDefault1"
            android:label="Icons1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.jakewharton.android.viewpagerindicator.sample.SAMPLE" />
            </intent-filter>
        </activity>

        <!-- ICON INDICATOR -->
        <activity
            android:name="com.lisb.viewpageindicator3.sample.SampleIconsDefault2"
            android:label="Icons2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.jakewharton.android.viewpagerindicator.sample.SAMPLE" />
            </intent-filter>
        </activity>

    </application>
</manifest>
